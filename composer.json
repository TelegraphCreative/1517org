{
  "require": {
    "craftcms/cms": "^3.0.0",
    "fortrabbit/craft-copy": "^1.0.0-RC4",
    "nystudio107/craft-minify": "1.2.9",
    "verbb/super-table": "2.1.16",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
