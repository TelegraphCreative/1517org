{# Link #}
{% macro link(source, classes, placeholderText) %}
    {% import _self as icon %}

    {% if source %}
        <a href="{{ source.url }}" class="{{ classes }}"{% if source.target %} target="{{ source.target }}" {% endif %}>{{ source.text }}</a>
    {% else %}
        <a href="#" class="{{ classes }}">
            {{ placeholderText }}
        </a>
    {% endif %}
    
{% endmacro %}

{# Link with Icon #}
{% macro iconLink(source, classes, iconName, placeholderText) %}
    {% import _self as icon %}

    {% if source %}
        <a href="{{ source.url }}" class="{{ classes }} -has-icon"{% if source.target %} target="{{ source.target }}" {% endif %}>
            <span class="align-items-center">
                {{ icon.utilityIcon(icon, '-xs') }}
                <span class="btn__text">
                    {{ source.text }}
                </span>
            </span>
        </a>
    {% else %}
        <a href="#" class="{{ classes }} -has-icon">
            <span class="align-items-center">
                {{ icon.utilityIcon(iconName, '-xs') }}
                <span class="btn__text">
                    {{ placeholderText }}
                </span>
            </span>
        </a>
    {% endif %}
{% endmacro %}


{# Image #}
{% macro image(source, class, placeholder) %}
    {# Source #}
    {% if class %}
        {% set classes = class %}
    {% else %}
        {% set classes = '' %}
    {% endif %}
    
    {% if source|length %}
        <figure class="image {{ classes }}">
            <img data-src="{{ source.src() }}"
                data-srcset="{{ source.srcset() }}"
                sizes="100vw"
                class="lazyload" />
        </figure>
    {% else %}
        {# Placeholder #}
        <figure class="image {{ classes }}">
            <img src="{{ placeholder }}">
        </figure>
    {% endif %}
{% endmacro %}

