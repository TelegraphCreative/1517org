{# Link #}
{% macro link(source, classes, placeholderText) %}
{% spaceless %}

    {% if source %}
        {# More options here: https://github.com/fruitstudios/craft-linkit/blob/master/README.md #}
        {% set attributes = {
            class: classes
        } %}
        {{ source.link(attributes) }}
    {% else %}
        <a href="#" class="{{ classes }}">
            {{ placeholderText }}
        </a>
    {% endif %}
    
{% endspaceless %}
{% endmacro %}

{# Link with Icon #}
{% macro iconLink(source, classes, iconName, iconSize, placeholderText) %}
{% spaceless %}
    {% import _self as icon %}

    {% if source %}
        <a href="{{ source.url }}" class="{{ classes }} -has-icon"{% if source.target %} target="{{ source.target }}" {% endif %}>
            <span class="align-items-center">
                {{ icon.utilityIcon(iconName, iconSize) }}
                <span class="btn__text">
                    {{ source.text }}
                </span>
            </span>
        </a>
    {% else %}
        <a href="#" class="{{ classes }} -has-icon">
            <span class="align-items-center">
                {{ icon.utilityIcon(iconName, '-md') }}
                <span class="btn__text">
                    {{ placeholderText }}
                </span>
            </span>
        </a>
    {% endif %}
{% endspaceless %}
{% endmacro %}

{# Image (autosizes) #}
{% macro image(source, classes, placeholder) %}
{% spaceless %}
    
    {% if classes is defined %}
        {% set classes = classes %}
    {% else %}
        {% set classes = '' %}
    {% endif %}

    {% set style = "padding-top: calc(" ~ source.height ~ " / " ~ source.width ~ " * 100%);" %}{% if source.mimeType != 'image/png' %}
        {% set style = style ~ " background: " ~ source.optimize.colorPalette[0] %}
    {% endif %}

    {# Source #}
    {% if source|length %}
        <figure class="image | {{ classes }}" style="{{ style }}">
            <img data-src="{{ source.optimize.src() }}"
                data-srcset="{{ source.optimize.srcset() }}"
                sizes="100vw"
                class="lazyload blur-up" 
                itemprop="image"/>
        </figure>

    {% else %}

        {# Static Placeholder #}
        <figure class="image {{ classes }}">
            <img src="{{ placeholder }}">
        </figure>

    {% endif %}

{% endspaceless %}
{% endmacro %}

{# Image (set sizes) #}
{% macro imageSetRatio(source, classes) %}
{% spaceless %}
    {# Source #}
    {% if source|length %}
        <figure class="image{% if classes is defined %} | {{ classes }}{% endif %}">
            <img data-src="{{ source.optimize.src() }}"
                data-srcset="{{ source.optimize.srcset() }}"
                sizes="100vw"
                class="lazyload blur-up" 
                itemprop="image"/>
        </figure>

    {% else %}

        {# Static Placeholder #}
        <figure class="image {{ classes }}">
            <img src="{{ placeholder }}">
        </figure>

    {% endif %}

{% endspaceless %}
{% endmacro %}

{# Card Image #}
{% macro cardImage(source, classes, placeholder) %}
{% spaceless %}
    
    {% if classes is defined %}
        {% set classes = classes %}
    {% else %}
        {% set classes = '' %}
    {% endif %}

    {# Source #}
    {% if source|length %}
        <figure class="image | {{ classes }}" style="background: {{ source.optimize.colorPalette[0] }}">
            <img data-src="{{ source.optimize.src() }}"
                data-srcset="{{ source.optimize.srcset() }}"
                sizes="100vw"
                class="lazyload" />
        </figure>
    {% else %}
        {# Static Placeholder #}
        <figure class="image {{ classes }}">
            <img src="{{ placeholder }}">
        </figure>
    {% endif %}

{% endspaceless %}
{% endmacro %}

{% macro coverImage(source, classes) %}
{% spaceless %}
    
    {% if classes is defined %}
        {% set classes = classes %}
    {% else %}
        {% set classes = '' %}
    {% endif %}
    
    {# Source #}
    {% if source|length %}
        <figure class="image -cover | {{ classes }}" style="background: {{ source.optimize.colorPalette[0] }}">
            <img data-src="{{ source.optimize.src() }}"
                data-srcset="{{ source.optimize.srcset() }}"
                sizes="100vw"
                class="lazyload" />
        </figure>
        {% else %}
        <figure class="image -cover | {{ classes }}" style="background: #B1C4BF;">
        </figure>
    {% endif %}

{% endspaceless %}
{% endmacro %}


{# Related Topics #}
{% macro relatedTopics(source, theme) %}
{% spaceless %}

    {% if source %}
        {# Note: Loop through actual topics from source #}
        {# <a href="{{ source.url }}" class="{{ classes }}"{% if source.target %} target="{{ source.target }}" {% endif %}>{{ source.text }}</a> #}
        <ul class="items-bar">
            {% for category in source %}
                <li>
                    <a href="{{ category.url }}" class="link -underline -sm {{ theme }}">
                        {{ category.title }}
                    </a>
                </li>
            {% endfor %}
        </ul>
    {% else %}
        {# Static Placeholder #}
        
        <ul class="items-bar">
            <li>
                <a href="#" class="link -underline -sm {{ theme }}">
                    Truth
                </a>
            </li>
            <li>
                <a href="#" class="link -underline -sm {{ theme }}">
                    Love and Longer Titles
                </a>
            </li>
        </ul>
        
    {% endif %}
    
{% endspaceless %}
{% endmacro %}



{# Device Mockups #}
{% macro devices(device, image) %}
{% spaceless %}
    {% switch device %}

        {% case "iphone" %}
            <div class="marvel-device iphone-x">
                <div class="notch">
                    {# <div class="camera"></div> #}
                    <div class="speaker"></div>
                </div>
                {# <div class="top-bar"></div> #}
                <div class="sleep"></div>
                <div class="bottom-bar"></div>
                <div class="volume"></div>
                <div class="overflow">
                    <div class="shadow shadow--tr"></div>
                    <div class="shadow shadow--tl"></div>
                    <div class="shadow shadow--br"></div>
                    <div class="shadow shadow--bl"></div>
                </div>
                <div class="inner-shadow"></div>
                <div class="screen" style="background: url({{ image }});">
                    <!-- Content goes here -->
                </div>
            </div>
        {% default %}
            <!-- Device not found --> 
    {% endswitch %}  

{% endspaceless %}
{% endmacro %}

